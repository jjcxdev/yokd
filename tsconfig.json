{
	"compilerOptions": {
		"outDir": "dist",
	  "target": "es2020",
	  "module": "esnext",
	  "moduleResolution": "node",
	  "allowJs": true,
	  "checkJs": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "sourceMap": true
	},
	"include": ["src/**/*"],
"exclude": [
  "node_modules",
  ".svelte-kit",
  "build",
  "dist",
  "static",
  "tests",
  "*.config.cjs",
  ".git",
  "coverage",
  "public",
  "eslint.config.js",
  "postcss.config.js", 
  "svelte.config.js"
]
  }